<Window x:Class="lab.work._4._4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab.work._4._4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Button1" Content="Button1" Background ="ForestGreen" Click ="Button1_Click" HorizontalAlignment="Left" Margin="56,73,0,0" VerticalAlignment="Top" Height="73" Width="245"/>
        <Button x:Name="Button2" Content="Button2" Background ="Azure" Click="Button2_Click" HorizontalAlignment="Left" Margin="342,42,0,0" VerticalAlignment="Top" Height="68" Width="177"/>
        <Button x:Name="Button3" Content="Button3" Background ="Chocolate"  Click="Button3_Click" HorizontalAlignment="Left" Margin="594,16,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="77" Width="174">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.389"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="Button4" Content="Button4" Background="Aqua" Click ="Button4_Click" HorizontalAlignment="Left" Margin="227,188,0,0" VerticalAlignment="Top" Height="222" Width="92"/>
        <Button x:Name="Button5" Content="Button5"  Background ="Black" Foreground ="White" Click ="Button5_Click" HorizontalAlignment="Left" Margin="440,151,0,0" VerticalAlignment="Top" Height="218" Width="194"/>

    </Grid>
</Window>
